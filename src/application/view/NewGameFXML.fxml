<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.NewGameController">
   <children>
   	  <Canvas id="newCanvas" fx:id="newCanvas" height="480.0" mouseTransparent="true" width="640.0" />
      <Button id="easyButton" fx:id="easyButton" layoutX="220.0" layoutY="200.0" mnemonicParsing="false" onMouseClicked="#on_Click" prefHeight="20.0" prefWidth="200.0" text="Cadet" />
      <Button id="normalButton" fx:id="normalButton" layoutX="220.0" layoutY="250.0" mnemonicParsing="false" onMouseClicked="#on_Click" prefHeight="20.0" prefWidth="200.0" text="Commander" />
      <Button id="hardButton" fx:id="hardButton" layoutX="220.0" layoutY="300.0" mnemonicParsing="false" onMouseClicked="#on_Click" prefHeight="20.0" prefWidth="200.0" text="Armageddon" />
      <Button id="backButton" fx:id="backButton" layoutX="50.0" layoutY="430.0" mnemonicParsing="false" onMouseClicked="#on_Click" prefHeight="24.0" prefWidth="100.0" text="Back" />
      <Label alignment="CENTER" layoutX="220.0" layoutY="100.0" mouseTransparent="true" prefHeight="32.0" prefWidth="200.0" text="Difficulty Level" textAlignment="CENTER" textFill="RED">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>

   </children>
</AnchorPane>
